<h1>Movies uploading files Practice</h1>
<div class="movies-wrapper">
    {{#each movies}}
    <div class="movie-item">
        <img src="{{this.imgPath}}" alt="this.title">
        <h4>{{this.title}}</h4>
        <p>{{this.description}}</p>
    </div>
    {{else}}
      <p>There are no movies yet! :(</p>
    {{/each}}
</div>